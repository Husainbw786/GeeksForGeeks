
#include <stdio.h>

int main() {
	int test,n,max,min,i;
	scanf("%d",&test);
	for(int k=0;k<test;k++)
	{
	    scanf("%d",&n);
	
	    int arr[101];
	
	    for(i=0;i<n;i++)
	    {
	        scanf("%d",&arr[i]);
	    }   
	    
	max = arr[0];
    min = arr[0];
	    
	     for(i=0;i<n;i++)
	      {
	        if(max<arr[i])
	        {
	            max = arr[i];
	            
	        }
	        if(min>arr[i])
	        {
	            min = arr[i];
	            
	        }
	       
	    }
	     printf("%d %d\n",max,min);
	}
	return 0;
}
