class Solution {
  public:
    int sumOfMatrix(int n, int m, vector<vector<int>> Grid) {
        // code here
        int i,j;
      int ans = 0;
        for(i=0;i<n;i++)
        {
           for(j=0;j<m;j++)
           {
               ans += Grid[i][j];
           }
        }
        return ans;
        
    }
};
